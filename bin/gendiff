#!/usr/bin/env php
<?php

use function Differ\Cli\getInput;
use function Differ\Differ\genDiff;

require_once __DIR__ . '/../src/Autoload.php';

[
    'pathToFile1' => $pathToFile1,
    'pathToFile2' => $pathToFile2,
    'format' => $format
] = getInput();

$diff = genDiff($pathToFile1, $pathToFile2, $format);
print_r($diff);